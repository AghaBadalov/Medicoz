                                <button class="search-btn"><span class="fa fa-search"></span></button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Sticky Header  -->
            <div class="sticky-header">
                <div class="auto-container">
                    <div class="main-box">
                        <div class="logo-box">
                            <div class="logo"><a href="#"><img src="~/uploads/settings/@(settings.FirstOrDefault(x=>x.Key=="HeaderLogo")?.Value)" alt="" title=""></a></div>
