                        </div>

                        <!-- Cart Btn -->
                        <button class="cart-btn"><i class="icon flaticon-shopping-cart"></i><span class="count">3</span></button>

                        <a href="#nav-mobile" class="mobile-nav-toggler navbar-trigger"><span class="fa fa-bars"></span></a>
